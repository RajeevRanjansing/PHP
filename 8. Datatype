   <?php 
    /*  int
        float
        string
        boolean
        array
        object
        null    */

        $a=100;  //int
        echo $a."<br>";

        $b=25.66; //float
        echo $b."<br>";

        $c="Hello World"; //string
        echo $c."<br>";

        $d=TRUE;     //boolean
        echo $d."<br>";

        $ar=array("Indore","Pune","Mumbai","Gujarat");    //array
        print_r($ar);

        $e=null;
        echo $e;

    ?>
    </center>
